package baloni;

import java.awt.*;

public class Luk extends Figura{
	
	private int x,y;//koordinate
	private int stranica,korak;
	Color boja;
	
	public Luk(Scena s, int xx, int yy, int str, int kor, Color b){ super(s); x=xx; y=yy; stranica=str; korak=kor; boja=b; }
	
	@Override
	public int getX() {
		return x;
	}

	@Override
	public int getY() {
		return y;
	}

	@Override
	public void crtaj() {

	}

}
